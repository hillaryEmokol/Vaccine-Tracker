<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>health</servlet-name>
        <servlet-class>org.health.health</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Vaccine</servlet-name>
        <servlet-class>org.health.Vaccine</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Administration</servlet-name>
        <servlet-class>org.health.Administration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Booking</servlet-name>
        <servlet-class>org.health.Booking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SysAdminnistration</servlet-name>
        <servlet-class>org.health.SysAdminnistration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>health</servlet-name>
        <url-pattern>/health</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Vaccine</servlet-name>
        <url-pattern>/Vaccine</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Administration</servlet-name>
        <url-pattern>/Administration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Booking</servlet-name>
        <url-pattern>/Booking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SysAdminnistration</servlet-name>
        <url-pattern>/SysAdminnistration</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
